@model List<User>

<h1>Usuarios</h1>
<table class="table">
	<thead>
		<tr>
			<th scope="col">Nombre</th>
			<th  scope="col">Correo</th>

		</tr>
	</thead>
	<tbody>
	@foreach (var user in Model)
	{
		<tr>
			<td scope="row">@Html.DisplayFor(m=>user.Name)</td>
			<td>@Html.DisplayFor(m=>user.Email)</td>
		</tr>
	}
	</tbody>
</table>

